  <ItemGroup>
    <None Update="Resources\Localizer.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>Localizer.cs</LastGenOutput>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Resources\Localizer.cs">
      <DependentUpon>Localizer.tt</DependentUpon>
    </Compile>
    <Compile Update="Resources\Text.en-US.Designer.cs">
      <DependentUpon>Text.en-US.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Text.zh-TW.Designer.cs">
      <DependentUpon>Text.zh-TW.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Resources\Text.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Text.zh-TW.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Text.resx">
      <Generator>ResXFileCodeGenerator</Generator>
